@using Common.Models
@inherits BaseComponent

@if (Data is not null)
{
    <Link Class=@Data.Class Url=@Data.Url Label=@Data.Text IsExternal=Data.IsExternal
        StartIcon=Data.StartIcon EndIcon=Data.EndIcon Color=@Color FontSize=@FontSize />
}

@code {
    [Parameter]
    public LinkData? Data { get; set; }
}
